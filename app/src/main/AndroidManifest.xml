<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.techacademy.satou.apiapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ApiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ApiApp">
        <activity
            android:name=".kuponactivity"
            android:label="@string/title_activity_kuponactivity"
            android:theme="@style/Theme.ApiApp.NoActionBar"></activity>
        <activity android:name=".WebViewActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>